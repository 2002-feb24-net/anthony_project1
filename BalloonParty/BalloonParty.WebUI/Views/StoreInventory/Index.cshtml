@model IEnumerable<BalloonParty.DataAccess.SQLData.StoreInventory>

@{
    ViewData["Title"] = "Index";
}

<h1>Stores Full Inventory</h1>

@* <p>
    <a asp-action="Create">Create New</a>
</p> *@
<table class="table">
