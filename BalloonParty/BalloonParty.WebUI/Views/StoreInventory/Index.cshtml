            <td>
                @* <a asp-action="Edit" asp-route-id="@item.SinventoryId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SinventoryId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SinventoryId">Delete</a> *@
